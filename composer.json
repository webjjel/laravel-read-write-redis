{
  "name": "webjjel/laravel-read-write-redis",
  "description": "Using Read/Write Redis connection for Laravel",
  "keywords": [
    "webjjel",
    "laravel-read-write-redis",
    "laravel"
  ],
  "homepage": "https://github.com/webjjel/laravel-read-write-redis",
  "license": "MIT",
  "authors": [
    {
      "name": "Gaphun Kim",
      "email": "webjjel@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : ">=7.1.3",
    "illuminate/cache": "^5.8|^6.0",
    "illuminate/contracts": "^5.8|^6.0",
    "illuminate/support": "^5.8|^6.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Webjjel\\ReadWriteRedis\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Webjjel\\ReadWriteRedis\\ReadWriteRedisServiceProvider"
      ]
    }
  }
}
